{
  "name": "react-tour-of-heroes",
  "version": "1.0.0",
  "description": "A React implementation of Angular 2 Tour of Heroes tutorial",
  "main": "index.js",
  "scripts": {
    "lite": "lite-server",
    "webpack": "cross-env NODE_ENV=production webpack --config webpack.config.js --watch --profile --colors",
    "clean": "rimraf dist",
    "prestart": "npm run clean",
    "start": "concurrently \"npm run webpack\" \"npm run lite\""
  },
  "keywords": [
    "react"
  ],
  "author": "Dan Halverson",
  "license": "MIT",
  "devDependencies": {
    "awesome-typescript-loader": "3.0.0-beta.18",
    "concurrently": "3.1.0",
    "cross-env": "3.1.4",
    "html-webpack-plugin": "2.26.0",
    "lite-server": "2.2.2",
    "rimraf": "2.5.4",
    "source-map-loader": "0.1.6",
    "typescript": "2.1.5",
    "webpack": "1.14.0"
  },
  "dependencies": {
    "@types/react": "15.0.1",
    "@types/react-dom": "0.14.20",
    "react": "15.4.2",
    "react-dom": "15.4.2"
  }
}
